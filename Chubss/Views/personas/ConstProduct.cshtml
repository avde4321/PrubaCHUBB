
@model List<Chubss.Models.ConsultaProduc>

    <div class="modal-body">
        <div class="row col-lg-12 col-sm-12 col-md-12">
            <div class="card-body">
                <table id="tblPerfiUser" class="table table-bordered table-striped" style="font-size:14px">
                    <thead>
                        <tr>
                            <th>ID Producto</th>
                            <th>Prima</th>
                        </tr>
                    </thead>
                    <tbody>
                        @if (Model != null)
                        {
                            foreach (var item in Model)
                            {
                                <tr>
                                    <td>@item.Descripcion</td>
                                    <td>@item.Prima</td>

                                </tr>
                            }
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

